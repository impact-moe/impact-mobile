<?xml version='1.0' encoding='utf-8'?>
<widget id="io.shaneduffy.impact" version="0.1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Impact</name>
    <icon src="/img/star-logo-square-128.png" />
    <description>
        Get up to date information, drop rates and builds for Genshin Impact.
    </description>
    <author email="shane@shaneduffy.io" href="https://shaneduffy.io">
        Shane Duffy
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
